	<script type="text/javascript">
		function mostrarMenu(){
			let menu = document.getElementById("menuTopo");
			if(menu.className === "menu")
				menu.className += " responsive";
			else
				menu.className = "menu";
	
			//if(menu.classList.contains("menu")) menu.classList.add("responsive")
			//else menu.classList.remove("responsive")
		}
